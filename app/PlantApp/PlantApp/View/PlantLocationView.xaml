             xmlns:maps="clr-namespace:Xamarin.Forms.Maps;assembly=Xamarin.Forms.Maps"
             x:Class="PlantApp.View.PlantLocationView">
    <ContentPage.Content>
        <StackLayout>
            <maps:Map IsShowingUser="True"
                      x:Name="map"
                      MapType="Satellite">
            </maps:Map>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>